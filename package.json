{
  "name": "MyElectronStarter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "electron .",
    "watch": "webpack --watch",
    "test": "mocha ./{src,test}/**/*.test.{ts,tsx}",
    "lint": "npm run lint:eslint && npm run lint:tsc && npm run lint:prettier",
    "lint:eslint": "eslint --ext '.js,.jsx,.ts,.tsx' .",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --list-different '**/*.{js,jsx,ts,tsx,css,json}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,json}'"
  },
  "dependencies": {
    "font-awesome": "4.7.0",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.0",
    "@types/react": "16.0.36",
    "@types/react-dom": "16.0.3",
    "css-loader": "0.28.9",
    "electron": "1.7.12",
    "electron-packager": "10.1.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.6",
    "mocha": "5.0.0",
    "prettier": "^1.10.2",
    "sinon": "4.2.2",
    "ts-loader": "3.4.0",
    "ts-node": "4.1.0",
    "typescript": "2.7.1",
    "typescript-eslint-parser": "^12.0.0",
    "url-loader": "^0.6.2",
    "webpack": "3.10.0",
    "webpack-node-externals": "1.6.0"
  }
}
